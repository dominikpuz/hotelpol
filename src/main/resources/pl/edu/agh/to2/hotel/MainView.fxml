    <TabPane prefHeight="400.0" prefWidth="800.0" tabClosingPolicy="UNAVAILABLE">
        <Tab fx:id="reservationsTab" text="Reservations">
            <fx:include fx:id="reservationsTabPage" source="ReservationOverviewPane.fxml"/>
        </Tab>
        <Tab fx:id="roomsTab" text="Rooms">
            <fx:include fx:id="roomsTabPage" source="RoomOverviewPane.fxml"/>
        </Tab>
    </TabPane>
</AnchorPane>
