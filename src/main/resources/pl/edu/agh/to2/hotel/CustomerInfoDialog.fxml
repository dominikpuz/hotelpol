<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<BorderPane stylesheets="@base_app_style.css" styleClass="main_app_container" prefHeight="200.0" prefWidth="400.0" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="pl.edu.agh.to2.hotel.presenter.customer.CustomerInfoDialog">
   <center>
      <GridPane BorderPane.alignment="CENTER">
        <columnConstraints>
          <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
          <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
        </columnConstraints>
        <rowConstraints>
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
        </rowConstraints>
         <children>
            <VBox alignment="CENTER" prefHeight="200.0" prefWidth="100.0">
               <children>
                  <Label styleClass="action_label" text="First name" />
                  <Label styleClass="action_label" fx:id="firstName" text="Text" />
               </children>
            </VBox>
            <VBox alignment="CENTER" layoutX="10.0" layoutY="10.0" prefHeight="200.0" prefWidth="100.0" GridPane.columnIndex="1">
               <children>
                  <Label styleClass="action_label" text="Last name" />
                  <Label styleClass="action_label" fx:id="lastName" text="Text" />
               </children>
            </VBox>
            <VBox alignment="CENTER" layoutX="210.0" layoutY="10.0" prefHeight="200.0" prefWidth="100.0" GridPane.rowIndex="1">
               <children>
                  <Label styleClass="action_label" text="Email" />
                  <Label styleClass="action_label" fx:id="email" text="Text" />
               </children>
            </VBox>
            <VBox alignment="CENTER" layoutX="10.0" layoutY="85.0" prefHeight="200.0" prefWidth="100.0" GridPane.columnIndex="1" GridPane.rowIndex="1">
               <children>
                  <Label styleClass="action_label" text="Phone number" />
                  <Label styleClass="action_label" fx:id="phoneNumber" text="Text" />
               </children>
            </VBox>
         </children>
      </GridPane>
   </center>
   <bottom>
       <HBox prefHeight="25.0" prefWidth="200.0" BorderPane.alignment="CENTER_RIGHT">
           <Button alignment="CENTER_RIGHT" mnemonicParsing="false" onAction="#handleOkAction" text="Ok" styleClass="action_button">
               <HBox.margin>
                   <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
               </HBox.margin>
           </Button>
       </HBox>
   </bottom>
</BorderPane>
