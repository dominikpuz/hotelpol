<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<VBox prefHeight="60.0" prefWidth="246.0" xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="pl.edu.agh.to2.hotel.presenter.customer.CustomerPickerSummary">
    <HBox alignment="CENTER_LEFT" prefHeight="100.0" prefWidth="100.0">
        <children>
            <Button mnemonicParsing="false" onAction="#handleSelectCustomer" text="Select customer">
                <HBox.margin>
                    <Insets bottom="5.0" right="5.0" top="5.0" />
                </HBox.margin>
            </Button>
            <Button mnemonicParsing="false" onAction="#handleNewCustomer" text="New customer">
                <HBox.margin>
                    <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
                </HBox.margin>
            </Button>
        </children>
    </HBox>
    <Text fx:id="customerSummary" strokeType="OUTSIDE" strokeWidth="0.0" text="Text">
        <HBox.margin>
            <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
        </HBox.margin>
    </Text>
</VBox>
